{
  "version": 2,
  "builds": [
    {
      "src": "Flask Deployed App/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "Flask Deployed App/static/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Flask Deployed App/templates/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Flask Deployed App/app.py"
    }
  ]
}
